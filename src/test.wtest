import example.*

test "obtengo nivel de hechizeria de Rolando" {
	assert.equals(3*17+5, rolando.nivelDeHechiceria())

}

test "Cambio a hechizo basico y obtengo nivel hechizeria" {
	rolando.cambiarHechizo(hechizoBasico)
	assert.equals(3*10+5, rolando.nivelDeHechiceria())

}

test "Provocar eclipse" {
	eclipse.hayEclipse()
	assert.equals(3*10+5, rolando.nivelDeHechiceria())
	
}

test "Cambiar nombre de espectro malefico" {
	assert.equals("Espectro buenifico", espectroMalefico.seCambiaElNombre("Espectro buenifico"))
}

